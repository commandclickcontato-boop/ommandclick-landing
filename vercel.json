{
  "buildCommand": "bun run export:web",
  "outputDirectory": "dist",
  "installCommand": "bun install",
  "devCommand": "bun run web",
  "routes": [
    {
      "src": "/_expo/static/(.*)",
      "dest": "/_expo/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
